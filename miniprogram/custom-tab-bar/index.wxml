<!--custom-tab-bar/index.wxml-->
<view class="tab-bar" style="height: {{TabBarHeight}}px; margin-bottom: {{BottomIndicatorHeight}}px;">
  <image
    src="{{selected === index ? item.selectedIconPath : item.iconPath}}"
    class="tab-bar-item tab-bar-item-icon"
    wx:for="{{list}}" wx:key="index"
    data-index="{{index}}"
    data-data="{{item}}"
    bindtap="switchTab"
    mode="widthFix"></image>
</view>
