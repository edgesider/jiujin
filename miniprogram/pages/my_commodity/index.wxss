.title-bar {
  width: 100%;
  height: auto;
  flex: 0 0 auto;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 1;
}

.list {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: stretch;
}

.card {
  border-bottom: 0.5px solid rgba(0, 0, 0, 0.2);
  padding: 0 12px;
  background: white;
  height: auto;
}
