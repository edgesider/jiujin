<view style="height: {{StatusBar}}px" class="top-space" />
<view class="top" style="height: {{CustomBar}}px; {{hasDivider ? 'border-bottom: 1rpx solid rgba(0, 0, 0, 0.1)' : ''}}">
  <text
    class="back {{isFirstPage ? 'cuIcon-home' : 'cuIcon-back'}}"
    wx:if="{{!isTabBarPage}}"
    bindtap="back"
  />
  <text class="title">{{title}}</text>
</view>
